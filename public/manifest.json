{
	"name": "Format Certifier",
	"description": "Automating certification of new ad formats",
	"version": "1.0",
	"manifest_version": 3,
	"action": {},
	"permissions": ["tabs", "scripting"],
	"background": {
		"service_worker": "serviceWorker.bundle.js"
	},
	"host_permissions": ["*://*/"],
	"content_security_policy": {
		"script-src": "self",
		"object-src": "self"
	},
	"web-accessible-resources": ["ui/index.html"],
	"icons": {
		"16": "logo16.png",
		"48": "logo48.png",
		"128": "logo128.png"
	}
}
